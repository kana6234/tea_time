<div class="container">
  <h4 class="row text-center page_title">ブックマーク一覧</h4>
  <div class="row ml-sm-4">
    <% @questions.each do |question| %>
      <div class="col-sm-5 mx-sm-3">
      <%= link_to question_path(question.id) do %>
        <%= render 'public/questions/list', question: question %>
      <% end %>
      </div>
    <% end %>
  </div>
</div>